{
  "name": "serverless",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "postinstall": "prisma generate",
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build && NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/config/neonconfig.ts",
    "orm-create": "npm run typeorm migration:create -- -n",
    "migrate": "npm run typeorm migration:run",
    "migration:rollback": "npm run typeorm migration:revert",
    "seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -n src/config/ormconfig.ts",
    "precommit": "lint-staged && npm run lint",
    "prepush": "npm run test:unit && npm run test:e2e",
    "prepare": "husky"
  },
  "dependencies": {
    "@doracoder/fcm-nestjs": "^9.0.5",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bullmq": "^10.2.2",
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.4.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.7",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/platform-socket.io": "^10.4.7",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.7",
    "@previewjs/analyzer-api": "^0.1.7",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.46.1",
    "@types/compression": "^1.7.5",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/ws": "^8.5.13",
    "@vercel/node": "^3.2.24",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.25.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "csrf-csrf": "^3.0.8",
    "ejs": "^3.1.10",
    "express-session": "^1.18.1",
    "mongoose": "^8.8.1",
    "mysql2": "^3.11.4",
    "nestjs-fcm": "^0.2.0",
    "nestjs-firebase": "^10.5.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.22.0",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/swagger": "^8.0.5",
    "@nestjs/testing": "^10.4.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.9.0",
    "@types/nodemailer": "^6.4.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "29.7.0",
    "multer": "^1.4.5-lts.1",
    "prettier": "^3.3.3",
    "sharp": "^0.33.5",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
